<app-layout vue-id="1" class="data-v-192dcee2" bind:__l="__l" vue-slots="{{['default']}}"><app-tab-nav vue-id="{{('2')+','+('1')}}" tabList="{{tabList}}" padding="0" activeItem="{{activeTab}}" theme="{{theme}}" data-event-opts="{{[['^click',[['tabStatus']]]]}}" bind:click="__e" class="data-v-192dcee2" bind:__l="__l"></app-tab-nav><view class="order main-center cross-center data-v-192dcee2"><view data-event-opts="{{[['tap',[['choose',['$event']]]]]}}" class="main-center cross-center data-v-192dcee2" bindtap="__e"><view class="data-v-192dcee2">{{active.name}}</view><image class="more data-v-192dcee2" src="../image/open.png"></image></view></view><view class="order-placeholder data-v-192dcee2"></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['toOrder',['$0'],[[['list','id',item.id,'order_no']]]]]]]}}" bindtap="__e" class="data-v-192dcee2"><view class="msg-item data-v-192dcee2"><block wx:if="{{activeTab==8}}"><view class="msg-title data-v-192dcee2"><text class="plugin-name data-v-192dcee2">{{item.plugin_name}}</text>有新订单啦</view></block><block wx:if="{{activeTab==4}}"><view class="msg-title data-v-192dcee2">退款申请</view></block><block wx:if="{{activeTab==1&&item.type==2}}"><view class="msg-title cross-center data-v-192dcee2"><text class="replace data-v-192dcee2"></text>换货申请</view></block><block wx:if="{{activeTab==1&&item.type==1}}"><view class="msg-title cross-center data-v-192dcee2"><text class="return data-v-192dcee2"></text>退货退款申请</view></block><view class="msg-time data-v-192dcee2">{{item.created_at}}</view><view class="goods-info data-v-192dcee2"><image class="goods-img data-v-192dcee2" src="{{activeTab==1?item.detail.goods_info.goods_attr.cover_pic:item.detail[0].goods.goodsWarehouse.cover_pic}}"></image><view class="goods-name t-omit-two data-v-192dcee2"><text class="data-v-192dcee2">{{activeTab==1?item.detail.goods_info.goods_attr.name:item.detail[0].goods.goodsWarehouse.name}}</text><block wx:if="{{item.detail.length>1&&activeTab!=1}}"><text class="data-v-192dcee2">{{"等"+item.detail.length+"件商品"}}</text></block></view><view class="goods-user data-v-192dcee2">{{"来自用户"+item.nickname}}</view></view></view></view></block><block wx:if="{{list.length===0&&!loading}}"><view class="no-tip data-v-192dcee2"><image src="{{adminImg.no_message}}" class="data-v-192dcee2"></image><view class="data-v-192dcee2">没有任何消息哦~</view></view></block><block wx:if="{{dialog}}"><view class="dialog data-v-192dcee2"><view class="picker-list data-v-192dcee2"><view class="main-between picker-header data-v-192dcee2"><view data-event-opts="{{[['tap',[['toggle',[false,2]]]]]}}" bindtap="__e" class="data-v-192dcee2">取消</view><view data-event-opts="{{[['tap',[['toggle',[false,1]]]]]}}" bindtap="__e" class="data-v-192dcee2">确定</view></view><picker-view indicator-style="height: 36px;" value="{{chooseIndex}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e" class="data-v-192dcee2"><picker-view-column class="data-v-192dcee2"><block wx:for="{{choose_list}}" wx:for-item="item" wx:for-index="idx" wx:key="name"><view class="picker-view data-v-192dcee2" style="{{'color:'+(newIndex==idx?'#446dfd':newIndex==idx+1||newIndex==idx-1?'#999999':newIndex==idx+2||newIndex==idx-2?'#cdcdcd':'')+';'}}">{{item.name+''}}</view></block></picker-view-column></picker-view></view></view></block></app-layout>